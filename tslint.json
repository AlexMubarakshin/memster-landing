{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "jsx-self-close": false,
    "member-access": false,
    "member-ordering": false,
    "no-console": false,
    "no-debugger": false,
    "no-empty-interface": false,
    "quotemark": true
  }
}
